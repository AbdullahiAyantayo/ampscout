{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "installCommand": "pip install --upgrade pip setuptools wheel && pip install -r requirements.txt",
  "framework": "fastapi",
  "outputDirectory": ".",
  "devCommand": "uvicorn main:app --host 0.0.0.0 --port $PORT",
  "build": {
    "env": {
      "PYTHON_VERSION": "3.9"
    }
  }
} 